  "scripts": {
    "start": "cd docs && pv",
    "global": "cd components/global && mp build",
    "button": "cd components/button && mp build",
    "card": "cd components/card && mp build",
    "description": "cd components/description && mp build",
    "error": "cd components/error && mp build",
    "photo": "cd components/photo && mp build",
    "timeline": "cd components/timeline && mp build",
    "user": "cd components/user && mp build",
    "input": "cd components/input && mp build",
    "build": "npm run global && npm run button && npm run card && npm run description && npm run error && npm run photo && npm run timeline && npm run user && npm run input",
